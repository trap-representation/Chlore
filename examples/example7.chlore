/ example 8: Recursive Fibonacci series
  Recursive Fibonacci series
/

import "./lib/import/io.chloreh"

:main
set n 10
pushl n
pushp fib
call

pushuc 0 pushp std_io call
pushuc 0 pushuc 0 pushp putl call
pushuc 0 pushuc 1 pushp putchar call
pushuc 0 invoke

pushc '\n'
pushuc 1 invoke

hlt

:fib
swap
dup pushuc 1 pushp return jge
dup pushuc 1 swap subl pushp fib call
swap
pushuc 2 swap subl pushp fib call
addl
swap
ret


:return
swap ret
ret

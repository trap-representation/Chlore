/ example 7: Recursive Fibonacci series
  Recursive Fibonacci series
/

import "stdlib.chloreh"
import "io.chloreh"

:main
  set n 37
  pushl n
  pushp fib
  call
  pushp putl call
  pushc '\n'
  pushp putchar call

  hlt

:fib
  swap
  dup pushi 1 pushp return jge
  dup pushi 1 swap subl pushp fib call
  swap
  pushi 2 swap subl pushp fib call
  addl
  swap
  ret

  :return
    swap
    ret

/ example 7: Formatted Strings
  Writing formatted strings to stdout
/

import "./lib/import/io.chloreh"

:main
pushuc 0
pushp std_io
call

pushuc 0
pushuc 0
pushp putfmt
call

pushp "the ultimate question of life, the universe, and everything."
pushui 42
pushp "%ui is the answer to %t\n"
pushuc 0 !

hlt

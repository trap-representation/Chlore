/ example 8: Recursive Fibonacci series
  Recursive Fibonacci series
/

import "stdlib.chloreh"
import "io.chloreh"

:main
set n 10
pushl n
pushp fib
call
pushp putl call
pushc '\n'
pushp putchar call

hlt

:fib
swap
dup pushuc 1 pushp return jge
dup pushuc 1 swap subl pushp fib call
swap
pushuc 2 swap subl pushp fib call
addl
swap
ret


:return
swap ret
ret
